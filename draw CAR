#include <stdio.h> 
#include <conio.h> 
#include <graphics.h> 
  
  
int main() 
{ 
    int gd = DETECT,gm;//graphics module 
    initgraph(&gd,&gm,"C:\\TURBOC\\BGI"); 
  
    //road 
    line(0,350,700,350); 
  
    //middle base 
    line(210,300,410,300); 
  
    //first wheel 
    circle(160,300,50); 
    circle(160,300,20); 
  
    //second wheel 
    circle(460,300,50); 
    circle(460,300,20); 
  
    //front base 
    line(110,300,10,300); 
  
    //back base 
    line(510,300,610,300); 
  
    //upper line above wheel 
    line(40,200,580,200); 
  
    //base front tilt line 
    line(10,300,40,200); 
  
    //base second tilt line 
    line(610,300,580,200); 
  
    //floor line 
    line(160,70,460,70); 
  
    //upper front tilt line 
    line(100,200,160,70); 
  
    //upper back tilt line 
    line(520,200,460,70); 
  
    //first window 
    int point1[] = {170,80,310,80,310,190,120,190,170,80}; 
    drawpoly(5,point1); 
    line(170,80,170,190); 
  
    //second window 
    int point2[] = {450,80,500,190,320,190,320,80,450,80}; 
    drawpoly(5,point2); 
    //line(450,80,450,190); 
  
    //first down door 
    int point3[] = {120,255,120,210,310,210,310,290,220,290}; 
    drawpoly(5,point3); 
    arc(160,300,9,131,60); 
    line(160,220,180,220); 
  
    //second down door 
    int point4[] = {500,255,500,210,320,210,320,290,400,290}; 
    drawpoly(5,point4); 
    arc(460,300,49,171,60); 
    line(330,220,350,220); 
    getch(); 
    return 0; 
} 
